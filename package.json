{
  "name": "cookie-omega-discord-bot",
  "version": "1.0.0",
  "description": "A bot that's powering Cookie Omega on Discord.",
  "repository": "Archomeda/cookie-omega-discord-bot",
  "author": "Archomeda",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "lint": "xo",
    "test": "echo \"Tests are currently not maintained\"",
    "cover": "echo \"Coverage is currently not maintained\""
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "common-tags": "^1.7.2",
    "deep-equal": "^1.0.1",
    "deepmerge": "^2.0.1",
    "discord.js": "^11.3.0",
    "discord.js-commando-plus": "archomeda/discord.js-commando-plus",
    "filesize": "^3.5.11",
    "i18next": "^10.2.2",
    "js-yaml": "^3.13.1",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "mongoose": "^5.0.2",
    "mongoose-sequence": "^4.0.1",
    "overwatch-api": "^0.7.0"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo": {
    "ignores": [
      "test/**/*"
    ],
    "space": 4,
    "rules": {
      "camelcase": "warn",
      "linebreak-style": "off",
      "no-div-regex": "off",
      "no-else-return": "warn",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ],
      "no-negated-condition": "off",
      "no-prototype-builtins": "off",
      "no-unused-vars": "warn",
      "no-use-extend-native/no-use-extend-native": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "unicorn/filename-case": "off"
    }
  }
}
